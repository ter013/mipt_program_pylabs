    "\n",
    "import turtle\n",
    "x=0\n",
    "y=0\n",
    "dt = 0.05\n",
    "Vy = 30\n",
    "Vx = 15\n",
    "for i in range(0,4000,1):\n",
    "    if y >=0:\n",
    "        x += Vx*dt\n",
    "        y += Vy*dt - 9.8/2*dt*dt\n",
    "        Vy += -9.8*dt\n",
    "        turtle.goto(x,y)\n",
    "    else:\n",
    "        y += 0.5\n",
    "        Vy = -Vy/1.1\n",
    "        Vx = Vx/1.1"
